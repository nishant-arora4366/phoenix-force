{
  "name": "phoenix-force-database-setup",
  "version": "1.0.0",
  "description": "Database setup and management tools for Phoenix Force",
  "main": "fetch-database-info.js",
  "scripts": {
    "fetch": "node fetch-database-info.js",
    "setup": "node setup-project.js",
    "setup:with-data": "node setup-project.js --with-sample-data",
    "setup:auto": "node setup-project.js --skip-confirmation",
    "backup": "node backup-data.js",
    "backup:json": "node backup-data.js --format=json",
    "backup:sql": "node backup-data.js --format=sql",
    "restore": "node restore-data.js",
    "restore:json": "node restore-data.js --source=json",
    "restore:sql": "node restore-data.js --source=sql",
    "restore:complete": "node restore-data.js --source=complete",
    "restore:dry-run": "node restore-data.js --dry-run",
    "docker:build": "docker-compose build phoenix-db-setup",
    "docker:backup": "./docker-backup.sh",
    "docker:backup:json": "./docker-backup.sh --format=json",
    "docker:backup:sql": "./docker-backup.sh --format=sql",
    "docker:restore": "./docker-restore.sh",
    "docker:restore:json": "./docker-restore.sh --source=json",
    "docker:restore:sql": "./docker-restore.sh --source=sql",
    "docker:restore:complete": "./docker-restore.sh --source=complete",
    "docker:restore:dry-run": "./docker-restore.sh --dry-run",
    "docker:setup": "./docker-setup.sh",
    "docker:setup:with-data": "./docker-setup.sh --with-data",
    "docker:local-db": "docker-compose --profile local-db up -d",
    "docker:local-db:down": "docker-compose --profile local-db down",
    "docker:local-db:logs": "docker-compose --profile local-db logs -f",
    "help": "echo 'Available commands: fetch, setup, setup:with-data, setup:auto, backup, backup:json, backup:sql, restore, restore:json, restore:sql, restore:complete, restore:dry-run, docker:build, docker:backup, docker:restore, docker:setup, docker:local-db'"
  },
  "keywords": [
    "supabase",
    "database",
    "setup",
    "phoenix-force",
    "cricket",
    "tournament"
  ],
  "author": "Phoenix Force Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nishant-arora4366/phoenix-force.git",
    "directory": "database-setup"
  }
}
